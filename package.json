{
    "name": "ProTally",
    "version": "1.3.1",
    "description": "Provides on-screen viewing of tally data. User can monitor status of up to 4 inputs in preview or program.",
    "keywords": [
        "util",
        "functional",
        "server",
        "client",
        "browser",
        "tsl",
        "tally",
        "carbonite",
        "graphhite",
        "blackmagic",
        "atem",
        "ross",
        "propresenter",
        "provideoserver",
        "obs"
    ],
    "license": "MIT",
    "author": "Joseph Adams",
    "dependencies": {
        "atem": "^0.1.8",
        "bonjour": "^3.5.0",
        "electron-notify": "^0.1.0",
        "electron-window-state-manager": "^0.3.2",
        "obs-websocket-js": "^1.2.0",
        "packet": "0.0.6",
        "tsl-umd": "^1.0.1"
    },
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=fixtures/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . protally --overwrite --asar=false --platform=win32 --arch=ia32 --icon=fixtures/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=JA --version-string.FileDescription=JA --version-string.ProductName=\"ProTally\"",
        "package-linux": "electron-packager . protally --overwrite --asar=true --platform=linux --arch=x64 --icon=fixtures/icons/png/1024x1024.png --prune=true --out=release-builds"
    },
    "devDependencies": {
        "electron": "^1.8.7"
    }
}
